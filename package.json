    "@primeuix/themes": "^1.2.5",
    "@tailwindcss/postcss": "^4.1.16",
    "angular-gridster2": "^20.2.3",
    "apexcharts": "^5.3.5",
    "ng-apexcharts": "^2.0.3",
    "primeicons": "^7.0.0",
    "primelocale": "^2.2.2",
    "primeng": "^20.3.0",
    "rxjs": "~7.8.0",
